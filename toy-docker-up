unset DAS_MONGODB_HOSTNAME
unset DAS_COUCHBASE_HOSTNAME

cp data/samples/animals.metta /tmp
cp toy-index.txt /tmp
docker stop `docker ps | grep 'das_app\|das_couchbase\|das_mongo' | cut -d' ' -f1`
docker rm `docker ps -a | grep 'das_app\|das_couchbase\|das_mongo' | cut -d' ' -f1`
docker volume rm das_couchbasedata
docker volume rm das_mongodbdata
docker-compose up -d
sleep 5
./scripts/couchbase_setup.sh
docker-compose exec app python das/das_upload_to_mongo.py -d TOY /tmp/animals.metta
sleep 5
docker-compose exec app python das/das_generate_file.py -d TOY --file-path /tmp/toy_all_pairs.txt --index-config /tmp/toy-index.txt
docker-compose exec app python das/das_upload_to_couch_from_file.py --file-path /tmp/toy_all_pairs.txt
docker-compose exec couchbase cbq --user dbadmin --password dassecret --script "CREATE PRIMARY INDEX ON default:das._default.Names" http://localhost:8091

